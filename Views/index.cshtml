@{
    Layout="Shared/_Layout.cshtml";
}

<h1>Your Hair Salon</h1>
@if(Model.Count <= 0)
{
    <h3>You don't have any stylists yet!</h3>
}
else
{
    <h2>Stylists: </h2>
    @foreach(var stylist in Model)
    {
        <h3><a href="">Name: @stylist.GetName()</a></h3>
    }
}

<h4><a href="/stylists/new">Click here to add a new stylist.</a></h4>
<h4><a href="/clients">Click here to view all clients.</a></h4>
